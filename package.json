    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.101.0",
