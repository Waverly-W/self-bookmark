{
  "name": "SelfBookmark",
  "description": "A Personal Bookmark Management Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["bookmarks", "storage"],
  "icons": {
    "16": "dist/icon_WH_16x16px.png",
    "48": "dist/icon_WH_48x48px.png",
    "128": "dist/icon_WH_128x128px.png"
  },
  "action": {
    "default_popup": "dist/popup.html",
    "default_icon": {
      "16": "dist/icon_WH_16x16px.png",
      "48": "dist/icon_WH_48x48px.png",
      "128": "dist/icon_WH_128x128px.png"
    }
  },
  "options_page": "dist/options.html"
}
